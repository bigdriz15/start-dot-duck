{
    "manifest_version": 2,
    "name": "Start Dot Duck",
    "short_name": "sdd",
    "description": "Adds the DuckDuckGo start page (start.duckduckgo.com) as a search engine to avoid that annoying pop-up",
    "version": "1.0",
    "author": "BigDriz",
    "developer": {
        "name": "Andrew Van Schaack",
        "url": "https://github.com/bigdriz15"
    },
    "homepage_url": "https://github.com/bigdriz15/start-dot-duck",
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "Start Dot Duck",
            "search_url": "https://start.duckduckgo.com?q={searchTerms}",
            "keyword": "@sdd",
            "favicon_url": "icons/icon.svg",
            "is_default": false,
            "encoding": "UTF-8"
        }
    },
    "icons": {
        "48": "icons/icon.svg",
        "64": "icons/icon.svg",
        "96": "icons/icon.svg",
        "144": "icons/icon.svg",
        "310": "icons/icon.svg",
        "540": "icons/icon.svg",
        "720": "icons/icon.svg",
        "1500": "icons/icon.svg"
    }
}